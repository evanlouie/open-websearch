{
  "name": "open-websearch",
  "version": "2.0.0",
  "description": "Multi-engine web search MCP server using Playwright browser automation (Bing, DuckDuckGo, Brave, Google) - zero API keys required",
  "type": "module",
  "bin": {
    "open-websearch": "./src/index.ts"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "start": "bun src/index.ts",
    "dev": "bun src/index.ts",
    "test": "bun test",
    "test:unit": "bun test src/__tests__/unit",
    "test:integration": "bun test src/__tests__/integration",
    "test:e2e": "bun test src/__tests__/e2e",
    "test:performance": "bun test src/__tests__/performance",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "inspector": "bunx @modelcontextprotocol/inspector src/index.ts",
    "mode:stdio": "MODE=stdio bun src/index.ts",
    "mode:http": "MODE=http bun src/index.ts",
    "mode:both": "MODE=both bun src/index.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.2",
    "playwright": "^1.55.1",
    "zod": "^4.0.0"
  },
  "module": "src/index.ts",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
